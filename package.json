{
  "name": "hirestime",
  "version": "6.0.0",
  "description": "thin wrapper around process.hrtime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p .",
    "test": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --require ts-node/register test.ts",
    "test:watch": "TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --require ts-node/register --watch test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/seriousManual/hirestime.git"
  },
  "keywords": [
    "hrtime",
    "hirestimer",
    "milliseconds",
    "seconds",
    "nanoseconds",
    "high resolution timer",
    "profiling",
    "profiler",
    "timing",
    "measurement"
  ],
  "author": "Manuel Ernst <seriousstuff@gmx.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seriousManual/hirestime/issues"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.12.12",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=6.0"
  }
}
